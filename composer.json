{
    "name": "vovchok/omeka-classic-sdk",
    "type": "library",
    "description": "An Omeka Classic SDK for PHP. Use the Omeka Classic API in your PHP projects.",
    "keywords": ["API", "Omeka", "Omeka Classic", "SDK"],
    "homepage": "https://github.com/vovchok/omeka-classic-sdk",
    "license": "MIT",
    "require": {
        "php": "^7.1",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^6.2",
        "symfony/serializer": "^3.1",
        "symfony/property-access": "^3.1",
        "symfony/http-client": "^4.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^8"
    },
    "authors": [
        {
            "name": "Volodymyr Vovchok",
            "email": "wovchok@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "OmekaClassic\\" : "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "OmekaClassicTest\\" : "tests/"
        }
    }
}
